#include <iostream>
#include <vector>

using namespace std;

int main(){
int count = 0;

    for(int a = 1; a < 20; a++){
        for(int b = 1; b < 20; b++){
            for(int c = 1; c < 20; c++){
                for(int d = 1; d < 20; d++){
                    if(d==4){
                        continue;
                    }
                    if(a+b+c+d == 20){
                        count++;
                    }
                }
            }
        }
    }

    cout<<count;
}
